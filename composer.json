{
    "name": "gnfsys/laravel-simple-model-validation",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "license": "MIT-0",
    "autoload": {
        "psr-4": {
            "GNfsys\\ModelValidation\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "larastan/larastan": "^3.9",
        "orchestra/testbench": "^10.9",
        "phpstan/extension-installer": "^1.4",
        "laravel/pint": "^1.27",
        "phpstan/phpstan-strict-rules": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
